	<!-- Projects -->
	<div class="portfolio-container">
		<div class="row">
			<div class="large-5 large-offset-2 columns">
				<h2 class="taxonomy-title">{{ object.labels.name }}</h2>
				<!--<p>{{# child_taxonomies }}{{^ first }}, {{/ first }}<a href="{{ permalink }}">{{ name }}</a>{{/ child_taxonomies }}-->
				{{# posts }}
					{{# post.even }}
					<div class="row single-container">
						<div class="large-12 columns img-container">
							<h2><a href="{{ post.permalink }}">{{ post.post_title }}</a></h2>
							<div class="entry-content">
								{{ post.post_excerpt }}
							</div>
							<div class="featured-img-container">
								<a href="{{ post.permalink }}">
									<img src="{{ post.featured_image.home_page }}" alt="{{ post.post_title }}">
								</a>
							</div>
						</div>
					</div>
					{{/ post.even}}
				{{/ posts }}
			</div>
			<div class="large-5 columns">
				{{# posts }}
					{{^ post.even }}
						<div class="row single-container">
							<div class="large-12 columns img-container">
								<h2><a href="{{ post.permalink }}">{{ post.post_title }}</a></h2>
								<div class="entry-content">
									{{ post.post_excerpt }}
								</div>
								<div class="featured-img-container">
									<a href="{{ post.permalink }}">
																			<img src="{{ post.featured_image.home_page }}" alt="{{ post.post_title }}">
																</a>
															</div>
							</div>
						</div>
					{{/ post.even }}
				{{/ posts }}
			</div>
		</div>

